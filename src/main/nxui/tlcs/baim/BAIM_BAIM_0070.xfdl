	// 운임적용구분
	this.fnAddCodeDef(this.dsFrtCalcDv, '선택');
	divFm.cboFrtCalcDv.set_index(0);
	
	// 전담구분
	this.fnAddCodeDef(this.dsFchrgDv, '선택');
	divFm.cboFchrgDv.set_index(0);
	
}

