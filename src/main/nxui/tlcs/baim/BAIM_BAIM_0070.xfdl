 * @function: fnCompInit
 * @description: 컴포넌트 초기화 함수.
 * @param  N/A
 * @return N/A
/***********************************************************************************************/
this.fnCompInit = function()
{
	// 발송구분
	this.fnAddCodeDef(this.dsSendDv, '선택');
	divFm.cboSendDv.set_index(0);
	
	// 택배상품명
	this.fnAddCodeDef(this.dsDlvGdsCd, '선택');
